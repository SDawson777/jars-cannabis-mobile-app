{
  "name": "backend",
  "version": "1.0.0",
  "description": "Express + Firestore backend for JARS Phase 4",
  "main": "dist/index.js",
  "engines": {
    "node": ">=18 <=22",
    "npm": ">=9"
  },
  "scripts": {
    "dev": "nodemon --watch src --ext ts --exec \"ts-node --project tsconfig.json src/index.ts\"",
    "build": "tsc -p tsconfig.json && tsc-alias -p tsconfig.json",
    "prestart": "npm run build && prisma migrate deploy",
    "start": "node dist/index.js",
    "mock": "prism mock openapi.yml --port 4000",
    "migrate": "prisma migrate deploy",
    "generate": "prisma generate",
    "postinstall": "prisma generate",
    "seed": "ts-node --transpile-only prisma/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.15.0",
    "@sentry/node": "^9.46.0",
    "bcryptjs": "^2.4.3",
    "bull": "^4.16.5",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "firebase-admin": "^12.7.0",
    "module-alias": "^2.2.3",
    "openai": "^4.104.0",
    "prisma": "^6.15.0",
    "stripe": "^14.25.0",
    "uuid": "^9.0.1",
    "helmet": "^7.2.0",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.23",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^18.19.124",
    "@types/uuid": "^9.0.8",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2"
  },
  "_moduleAliases": {
    "@server": "src"
  },
  "type": "commonjs"
}
