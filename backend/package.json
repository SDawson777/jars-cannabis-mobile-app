{
  "name": "backend",
  "version": "1.0.0",
  "description": "Express + Firestore backend for JARS Phase 4",
  "main": "dist/index.js",
  "engines": {
    "node": ">=18.18 <19"
  },
  "scripts": {
    "dev": "nodemon --watch src --ext ts --exec \"ts-node --project tsconfig.json src/index.ts\"",
    "build": "tsc -p tsconfig.json && tsc-alias -p tsconfig.json",
    "prestart": "prisma migrate deploy",
    "start": "node start-prod.js",
    "mock": "prism mock openapi.yml --port 4000",
    "migrate": "prisma migrate deploy",
    "generate": "prisma generate",
    "postinstall": "prisma generate"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^4.16.2",
    "prisma": "^4.16.2",
    "@sentry/node": "^9.40.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.18.2",
    "firebase-admin": "^11.0.0",
    "stripe": "^14.0.0",
    "uuid": "^9.0.1",
    "bull": "^4.12.2",
    "module-alias": "^2.2.3",
    "openai": "^4.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^18.7.18",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5",
    "@types/uuid": "^9.0.7",
    "@types/bull": "^4.10.4",
    "tsconfig-paths": "^4.2.0",
    "tsc-alias": "^1.8.8"
  },
  "_moduleAliases": {
    "@server": "src"
  }
}
