{
  "name": "jars-cannabis-mobile-app",
  "version": "1.0.0",
  "private": true,
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "clean:modules": "rm -rf node_modules package-lock.json && npm cache verify",
    "fix:deps": "bash scripts/fix-dependencies.sh",
    "prepare": "husky install",
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:web": "expo build:web",
    "vercel-build": "mkdir -p dist && echo '<!DOCTYPE html><html><head><meta charset=\"utf-8\"><title>Placeholder</title></head><body></body></html>' > dist/index.html",
    "build:backend": "npm --prefix backend run build",
    "start:backend": "npm --prefix backend run start",
    "build": "npm run build:backend",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write .",
    "format:fix": "prettier --write \"**/*.{ts,tsx,js,json,jsonc,json5,md}\"",
    "check-assets": "npx ts-node --esm scripts/checkAssets.ts",
    "firebase:check": "ts-node scripts/verifyFirebaseSetup.ts",
    "test:e2e:firebase": "jest tests/firebase.e2e.ts --runInBand",
    "dev:expo": "expo start",
    "dev:server": "nodemon src/index.ts",
    "dev:emulators": "firebase emulators:start --only firestore,functions,auth",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "integrate-terpene-assets": "mkdir -p assets/terpene-wheel && cp Terpene_Wheel_Code_Kit.zip assets/terpene-wheel && unzip -o assets/terpene-wheel/Terpene_Wheel_Code_Kit.zip -d assets/terpene-wheel",
    "scaffold:award-redemption": "ts-node scripts/setupAwardRedemption.ts",
    "test:award-redemption": "jest tests/awards.redeem.test.ts --runInBand",
    "prisma-gen": "npx prisma generate --schema=backend/prisma/schema.prisma || npx prisma generate --schema=backend/prisma/schema.prisma --skip-download",
    "smoke": "newman run qa/collection.json -e qa/env.staging.json --reporters cli,junit --reporter-junit-export ./reports/newman-smoke.xml",
    "format:check": "prettier --check .",
    "postinstall": "node scripts/make-splash.js"
  },
  "lint-staged": {
    "{*,src/**,!src/terpene_wheel/snippets/**}/*.{js,jsx,ts,tsx}": [
      "eslint --fix --quiet",
      "prettier --write"
    ],
    "*.{json,jsonc,json5,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^5.1.6",
    "@hookform/resolvers": "^5.2.1",
    "@prisma/client": "^6.12.0",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/datetimepicker": "8.4.1",
    "@react-native-community/netinfo": "^11.3.0",
    "@react-native-firebase/analytics": "^22.4.0",
    "@react-native-firebase/app": "^22.4.0",
    "@react-native-firebase/messaging": "^22.4.0",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.9.12",
    "@react-navigation/stack": "^6.3.16",
    "@sentry/node": "^9.40.0",
    "@sentry/react-native": "^6.19.0",
    "@stripe/stripe-react-native": "^0.39.0",
    "@tanstack/react-query": "^5.83.0",
    "analytics-node": "^6.2.0",
    "async-mutex": "^0.5.0",
    "aws-amplify": "^6.15.5",
    "axios": "^1.10.0",
    "bcrypt": "^6.0.0",
    "bull": "^4.16.5",
    "expo": "^53.0.20",
    "expo-av": "^15.1.7",
    "expo-haptics": "^14.1.4",
    "expo-local-authentication": "^16.0.5",
    "expo-localization": "^16.1.6",
    "expo-location": "~18.1.6",
    "expo-notifications": "^0.31.4",
    "expo-secure-store": "^12.4.0",
    "expo-speech": "^13.1.6",
    "expo-task-manager": "^11.6.0",
    "expo-web-browser": "^14.2.0",
    "express": "^5.1.0",
    "firebase": "^12.0.0",
    "firebase-admin": "^13.4.0",
    "firebase-functions": "^6.4.0",
    "formik": "^2.4.6",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "lottie-react-native": "^6.3.1",
    "lucide-react-native": "^0.525.0",
    "nativewind": "^4.1.23",
    "prisma": "^6.12.0",
    "react": "^19.1.0",
    "react-hook-form": "^7.62.0",
    "react-native-animatable": "^1.4.0",
    "react-native-confetti-cannon": "^1.3.2",
    "react-native-dotenv": "^3.4.11",
    "react-native-gesture-handler": "^2.27.2",
    "react-native-haptic-feedback": "^2.3.3",
    "react-native-linear-gradient": "^2.8.0",
    "react-native-maps": "^1.24.10",
    "react-native-pager-view": "^6.3.7",
    "react-native-reanimated": "^3.19.0",
    "react-native-safe-area-context": "^5.5.2",
    "react-native-screens": "^4.12.0",
    "react-native-shimmer-placeholder": "^2.0.7",
    "react-native-skeleton-placeholder": "^5.2.4",
    "react-native-sound": "^0.11.2",
    "react-native-svg": "15.11.2",
    "rtl-detect": "^1.1.2",
    "yup": "^1.6.1",
    "zustand": "^4.4.1"
  },
  "devDependencies": {
    "@eslint/css": "^0.10.0",
    "@eslint/js": "^9.33.0",
    "@eslint/json": "^0.13.0",
    "@stoplight/prism-cli": "^5.14.2",
    "@testing-library/react": "^16.3.0",
    "@types/aws-lambda": "^8.10.152",
    "@types/bcrypt": "^6.0.0",
    "@types/bull": "^3.15.9",
    "@types/express": "^5.0.3",
    "@types/hammerjs": "^2.0.46",
    "@types/istanbul-lib-report": "^3.0.3",
    "@types/istanbul-reports": "^3.0.4",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.2.1",
    "@types/react": "^19.1.9",
    "@types/react-native-dotenv": "^0.2.2",
    "@types/react-test-renderer": "^19.1.0",
    "@types/request": "^2.48.13",
    "@types/superagent": "^8.1.9",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "babel-jest": "^30.0.5",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsonc": "^2.20.1",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-native": "^5.0.0",
    "firebase-tools": "^14.11.0",
    "globals": "^16.3.0",
    "husky": "^8.0.3",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "jest-expo": "^53.0.9",
    "jsonc-eslint-parser": "^2.4.0",
    "lint-staged": "^13.2.3",
    "newman": "^6.2.1",
    "prettier": "^3.3.3",
    "react-dom": "^19.1.0",
    "react-test-renderer": "^19.1.1",
    "supertest": "^7.1.3",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=18 <=22",
    "npm": "11.4.2"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "@prisma/client",
          "@sentry/node",
          "analytics-node",
          "async-mutex",
          "bcrypt",
          "bull",
          "express",
          "firebase",
          "firebase-admin",
          "firebase-functions",
          "ioredis",
          "jsonwebtoken",
          "prisma",
          "rtl-detect"
        ]
      }
    }
  },
  "overrides": {
    "metro": "0.80.10",
    "metro-config": "0.80.10",
    "metro-core": "0.80.10",
    "metro-runtime": "0.80.10",
    "metro-resolver": "0.80.10",
    "metro-source-map": "0.80.10",
    "metro-cache": "0.80.10",
    "metro-minify-terser": "0.80.10"
  }
}
