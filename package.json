{
  "name": "jars-cannabis-mobile-app",
  "version": "1.0.0",
  "private": true,
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "prepare": "husky install",
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:web": "expo build:web",
    "vercel-build": "mkdir -p dist && echo '<!DOCTYPE html><html><head><meta charset=\"utf-8\"><title>Placeholder</title></head><body></body></html>' > dist/index.html",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --check \"**/*.{ts,tsx,js,json,jsonc,json5,md}\"",
    "format:fix": "prettier --write \"**/*.{ts,tsx,js,json,jsonc,json5,md}\"",
    "check-assets": "npx ts-node --esm scripts/checkAssets.ts",
    "firebase:check": "ts-node scripts/verifyFirebaseSetup.ts",
    "test:e2e:firebase": "jest tests/firebase.e2e.ts --runInBand",
    "dev:expo": "expo start",
    "dev:server": "nodemon src/index.ts",
    "dev:emulators": "firebase emulators:start --only firestore,functions,auth",
    "typecheck": "tsc --noEmit",
    "test": "jest"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --quiet",
      "prettier --write"
    ],
    "*.{json,jsonc,json5,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@prisma/client": "^6.12.0",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/datetimepicker": "8.4.1",
    "@react-native-firebase/app": "^22.4.0",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.9.12",
    "@react-navigation/stack": "^6.3.16",
    "@sentry/node": "^9.40.0",
    "@tanstack/react-query": "^5.83.0",
    "axios": "^1.10.0",
    "bcrypt": "^6.0.0",
    "bull": "^4.16.5",
    "expo": "^53.0.19",
    "expo-haptics": "^14.1.4",
    "expo-localization": "^16.1.6",
    "expo-location": "~18.1.6",
    "expo-web-browser": "^14.2.0",
    "express": "^5.1.0",
    "firebase": "^12.0.0",
    "firebase-admin": "^13.4.0",
    "firebase-functions": "^6.4.0",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "lucide-react-native": "^0.525.0",
    "nativewind": "^4.1.23",
    "prisma": "^6.12.0",
    "react": "^19.1.0",
    "react-native-dotenv": "^3.4.11",
    "react-native-haptic-feedback": "^2.3.3",
    "react-native-maps": "^1.24.10",
    "react-native-reanimated": "^3.18.0",
    "react-native-safe-area-context": "^5.5.2",
    "react-native-screens": "^4.12.0",
    "react-native-svg": "15.11.2",
    "rtl-detect": "^1.1.2"
  },
  "devDependencies": {
    "@eslint/css": "^0.10.0",
    "@eslint/js": "^9.31.0",
    "@eslint/json": "^0.13.0",
    "@stoplight/prism-cli": "^5.14.2",
    "@types/bcrypt": "^6.0.0",
    "@types/bull": "^3.15.9",
    "@types/express": "^5.0.3",
    "@types/hammerjs": "^2.0.46",
    "@types/istanbul-lib-report": "^3.0.3",
    "@types/istanbul-reports": "^3.0.4",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.0.15",
    "@types/react": "^19.1.8",
    "@types/react-native": "^0.73.0",
    "@types/react-native-dotenv": "^0.2.2",
    "@types/react-test-renderer": "^19.1.0",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsonc": "^2.20.1",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-native": "^5.0.0",
    "firebase-tools": "^14.11.0",
    "globals": "^16.3.0",
    "husky": "^9.0.0",
    "jest": "^30.0.4",
    "jsonc-eslint-parser": "^2.4.0",
    "lint-staged": "^14.0.0",
    "prettier": "^3.6.2",
    "react-test-renderer": "^19.1.0",
    "supertest": "^7.1.3",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}