{
  "name": "jars-cannabis-mobile-app",
  "version": "1.0.0",
  "private": true,
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "demo:web": "cross-env VITE_API_BASE_URL=http://localhost:8080/api/v1 npm --prefix apps/demo-web run dev",
    "clean:modules": "rm -rf node_modules package-lock.json && npm cache verify",
    "fix:deps": "bash scripts/fix-dependencies.sh",
    "prepare": "sh -c 'if [ -n \"$CI\" ]; then exit 0; fi; if [ -x ./node_modules/.bin/husky ]; then ./node_modules/.bin/husky install; fi'",
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:web": "expo build:web",
    "vercel-build": "mkdir -p dist && echo '<!DOCTYPE html><html><head><meta charset=\"utf-8\"><title>Placeholder</title></head><body></body></html>' > dist/index.html",
    "build:backend": "npm --prefix backend run build",
    "start:backend": "npm --prefix backend run start",
    "build": "npm run build:backend",
    "precommit": "npm run lint && npm run typecheck && npm run test:ci",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --cache",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --cache --fix",
    "format": "prettier --write .",
    "format:ci": "prettier --check .",
    "format:fix": "prettier --write \"**/*.{ts,tsx,js,json,jsonc,json5,md}\"",
    "check-assets": "node scripts/checkAssets.js",
    "firebase:check": "ts-node scripts/verifyFirebaseSetup.ts",
    "test:e2e:firebase": "jest tests/firebase.e2e.ts --runInBand",
    "test:e2e": "detox test --configuration android.emu.debug",
    "test:e2e:ios": "detox test --configuration ios.sim.debug",
    "test:e2e:android": "detox test --configuration android.emu.debug",
    "build:e2e:android": "detox build --configuration android.emu.debug",
    "build:e2e:ios": "detox build --configuration ios.sim.debug",
    "dev:expo": "expo start",
    "dev:server": "nodemon src/index.ts",
    "dev:emulators": "firebase emulators:start --only firestore,functions,auth",
    "typecheck": "tsc --noEmit",
    "test": "NODE_OPTIONS=--max_old_space_size=4096 jest --config jest.config.cjs",
    "test:watch": "NODE_OPTIONS=--max_old_space_size=4096 jest --config jest.config.cjs --watch",
    "test:ci": "NODE_OPTIONS=--max_old_space_size=4096 jest --config jest.config.cjs --coverage --ci --watchAll=false",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "integrate-terpene-assets": "mkdir -p assets/terpene-wheel && cp Terpene_Wheel_Code_Kit.zip assets/terpene-wheel && unzip -o assets/terpene-wheel/Terpene_Wheel_Code_Kit.zip -d assets/terpene-wheel",
    "scaffold:award-redemption": "ts-node scripts/setupAwardRedemption.ts",
    "test:award-redemption": "jest tests/awards.redeem.test.ts --runInBand",
    "prisma-gen": "npx prisma generate --schema=backend/prisma/schema.prisma || npx prisma generate --schema=backend/prisma/schema.prisma --skip-download",
    "smoke": "newman run qa/collection.json -e qa/env.staging.json --reporters cli,junit --reporter-junit-export ./reports/newman-smoke.xml",
    "format:check": "prettier --check .",
    "postinstall": "node scripts/make-splash.js",
    "align-deps": "bash scripts/align-deps.sh"
  },
  "lint-staged": {
    "{*,src/**,!src/terpene_wheel/snippets/**}/*.{js,jsx,ts,tsx}": [
      "eslint --fix --quiet",
      "prettier --write"
    ],
    "*.{json,jsonc,json5,md}": [
      "prettier --write"
    ]
  },
